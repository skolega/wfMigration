<!DOCTYPE html>
<html lang="pl">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <!-- Meta, title, CSS, favicons, etc. -->
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <title>Gentallela Alela! | </title>

        <!-- Bootstrap -->
        <link href="{{ asset('assets/vendors/bootstrap/dist/css/bootstrap.min.css') }}" rel="stylesheet">
        <!-- Font Awesome -->
        <link href="{{ asset('assets/vendors/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet">
        <!-- iCheck -->
        <link href="{{ asset('assets/vendors/iCheck/skins/flat/green.css') }}" rel="stylesheet">
        <!-- bootstrap-progressbar -->
        <link href="{{ asset('assets/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css') }}" rel="stylesheet">
        <!-- jVectorMap -->
        <link href="{{ asset('assets/css/maps/jquery-jvectormap-2.0.3.css') }}" rel="stylesheet"/>
        <!-- myCss -->
        <link href="{{ asset('assets/css/myCss.css') }}" rel="stylesheet"/>
        <!-- Custom Theme Style -->
        <link href="{{ asset('assets/css/custom.min.css') }}" rel="stylesheet">
    </head>

    <body class="nav-md">
        <div class="container body">
            <div class="main_container">
                <div class="col-md-3 left_col">
                    <div class="left_col scroll-view">
                        <div class="navbar nav_title" style="border: 0;">
                            <a href="{{ path('app_gentelella', {'page': 'index'}) }}" class="site_title"><i class="fa fa-rocket"></i> <span>Rocket Mirodor!</span></a>
                        </div>

                        <div class="clearfix"></div>

                        <!-- menu profile quick info -->
                        <div class="profile">
                            <div class="profile_pic">
                                {% if app.user %}
                                    <img src="{{ asset('assets/images/avatars/'~ app.user.getAvatar() ~'.jpg') }}" alt="..." class="img-circle profile_img">
                                {% endif %}
                            </div>
                            <div class="profile_info">
                                <span>Witaj,</span>
                                <h2>{{ app.user |capitalize }}</h2>
                            </div>
                        </div>
                        <!-- /menu profile quick info -->

                        <br />
                        {% block sidebar %}
                            {% include 'admin/sidebar.html.twig' %}
                        {% endblock %}
                        {% block footer %}
                            {% include 'admin/footer.html.twig' %}
                        {% endblock %}



                    </div>
                </div>

                <!-- top navigation -->
                <div class="top_nav">
                    <div class="nav_menu">
                        <nav class="" role="navigation">
                            <div class="nav toggle">
                                <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                            </div>
                            <a href="{{ path('app_dashboard') }}" style="margin-top: 10px;" class="btn btn-success">Powr√≥t do aplikacji</a>

                            <ul class="nav navbar-nav navbar-right">
                                <li class="">
                                    <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                        {% if app.user %}
                                            <img src="{{ asset('assets/images/avatars/'~ app.user.getAvatar() ~'.jpg') }}" alt="">{{ app.user |capitalize }}
                                            <span class=" fa fa-angle-down"></span>
                                        {% endif %}
                                    </a>
                                    <ul class="dropdown-menu dropdown-usermenu pull-right">
                                        <li><a href="javascript:;"> Profil</a></li>
                                        <li>
                                            <a href="javascript:;">
                                                <span class="badge bg-red pull-right">50%</span>
                                                <span> Ustawienia</span>
                                            </a>
                                        </li>
                                        <li><a href="javascript:;">Pomoc</a></li>
                                            {% if app.user %}
                                            <li><a href="{{ path('fos_user_security_logout') }}"><i class="fa fa-sign-out pull-right"></i> Wyloguj</a></li>
                                            {% else %}
                                            <li><a href="{{ path('app_gentelella', {'page': 'login'}) }}"><i class="fa fa-sign-out pull-right"></i> Zaloguj</a></li>
                                            {% endif %}
                                    </ul>
                                </li>
                                {% include '/admin/msg_notices.html.twig' %}

                            </ul>
                        </nav>
                    </div>
                </div>
                <!-- /top navigation -->

                <!-- page content -->
                <div class="right_col" role="main">
                    {% block body %}
                        <!-- top tiles -->
                        {% include 'admin/top_titles.html.twig' %}
                        <!-- /top tiles -->
                        {% include 'admin/index_body.html.twig' %}
                    {% endblock %}
                </div>
                <!-- /page content -->

                <!-- footer content -->
                <footer>
                    <div class="pull-right">
                        Mirodor - Rocket Admin
                    </div>
                    <div class="clearfix"></div>
                </footer>
                <!-- /footer content -->
            </div>
        </div>
        {% block js %}
            <!-- jQuery -->
            <script src="{{ asset('assets/vendors/jquery/dist/jquery.min.js') }}"></script>
            <!-- Bootstrap -->
            <script src="{{ asset('assets/vendors/bootstrap/dist/js/bootstrap.min.js') }}"></script>
            <!-- FastClick -->
            <script src="{{ asset('assets/vendors/fastclick/lib/fastclick.js') }}"></script>
            <!-- NProgress -->
            <script src="{{ asset('assets/vendors/nprogress/nprogress.js') }}"></script>
            <!-- Chart.js -->
            <script src="{{ asset('assets/vendors/Chart.js/dist/Chart.min.js') }}"></script>
            <!-- gauge.js -->
            <script src="{{ asset('assets/vendors/gauge.js/dist/gauge.min.js') }}"></script>
            <!-- bootstrap-progressbar -->
            <script src="{{ asset('assets/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js') }}"></script>
            <!-- iCheck -->
            <script src="{{ asset('assets/vendors/iCheck/icheck.min.js') }}"></script>
            <!-- Skycons -->
            <script src="{{ asset('assets/vendors/skycons/skycons.js') }}"></script>
            <!-- Flot -->
            <script src="{{ asset('assets/vendors/Flot/jquery.flot.js') }}"></script>
            <script src="{{ asset('assets/vendors/Flot/jquery.flot.pie.js') }}"></script>
            <script src="{{ asset('assets/vendors/Flot/jquery.flot.time.js') }}"></script>
            <script src="{{ asset('assets/vendors/Flot/jquery.flot.stack.js') }}"></script>
            <script src="{{ asset('assets/vendors/Flot/jquery.flot.resize.js') }}"></script>
            <!-- Flot plugins -->
            <script src="{{ asset('assets/js/flot/jquery.flot.orderBars.js') }}"></script>
            <script src="{{ asset('assets/js/flot/date.js') }}"></script>
            <script src="{{ asset('assets/js/flot/jquery.flot.spline.js') }}"></script>
            <script src="{{ asset('assets/js/flot/curvedLines.js') }}"></script>
            <!-- jVectorMap -->
            <script src="{{ asset('assets/js/maps/jquery-jvectormap-2.0.3.min.js') }}"></script>
            <!-- bootstrap-daterangepicker -->
            <script src="{{ asset('assets/js/moment/moment.min.js') }}"></script>
            <script src="{{ asset('assets/js/datepicker/daterangepicker.js') }}"></script>
            <!-- my-app -->
            <script src="{{ asset('assets/js/myApp.js') }}"></script>
            <!-- Custom Theme Scripts -->
            <script src="{{ asset('assets/js/custom.min.js') }}"></script>
        {% endblock %}
        <!-- Flot -->
        {% block scripts %}
            {% include 'admin/scripts_mainpage.html.twig' %}
        {% endblock %}
        <!-- /gauge.js -->
    </body>
</html>
